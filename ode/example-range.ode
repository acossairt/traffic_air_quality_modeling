# example of fooling xpp into doing range over 2
# in silent mode which can only range in 1
table p1 % 25 0 24 t/25
table p2 % 25 0 24 t/25
par index
!a=2.3+.8*p1(mod(index,25))
!b=-3.4+6*p2(flr(index/25))
aux aa=a
aux bb=b
x'=-x+a
y'=-y+b*x
@ total=20,trans=19.999
@ range=1,rangeover=index
@ rangelow=0,rangehigh=624,rangestep=624
@ rangereset=no
done